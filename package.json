{
  "name": "apigateway",
  "version": "0.0.1",
  "description": "",
  "author": "rhkdguskim <rhkdguskim@naver.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && nest build authentication && nest build authorization",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev:authentication": "nest start authentication --watch",
    "start:dev:authorization": "nest start authorization --watch",
    "start:debug:friend": "nest start friend --watch",
    "start:dev:room": "nest start room --watch",
    "start:dev:chat": "nest start chat --watch",
    "start:dev:file": "nest start file --watch",
    "list": "pm2 list",
    "stop": "pm2 stop all",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/apigateway/test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.3",
    "@grpc/proto-loader": "^0.7.10",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^9.4.3",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.4",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^9.0.0",
    "@nestjsplus/cookies": "^1.1.1",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/config": "^3.3.3",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-kakao": "^1.0.0",
    "@types/passport-naver": "^1.0.1",
    "@types/socket.io-client": "^3.0.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "common-grpc": "file:libs/common",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "mysql2": "^3.5.2",
    "nest-winston": "^1.9.3",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-naver": "^1.0.6",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io-client": "^4.7.2",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.158.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@app/authentication(|/.*)$": "<rootDir>/apps/authentication/src/$1",
      "^@app/authorization(|/.*)$": "<rootDir>/apps/authorization/src/$1",
      "^@app/chat(|/.*)$": "<rootDir>/apps/chat/src/$1",
      "^@app/user(|/.*)$": "<rootDir>/apps/user/src/$1",
      "^@app/file(|/.*)$": "<rootDir>/apps/file/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ]
  },
  "repository": "rhkdguskim/chattingserver",
  "auto": {
    "plugins": [
      "npm"
    ]
  }
}
